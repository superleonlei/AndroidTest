<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.androidtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activities.AboutActivitiesListActivity"/>
        <activity android:name=".activities.LifeCircleAActivity"/>
        <activity android:name=".activities.LifeCircleBActivity"/>

        <activity android:name=".views.AboutViewsListActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".views.dispatch.DispatchEventTestActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".views.dispatch.ScrollViewTouchTestActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".views.aes_rsa.EncryptTestActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".views.aes_rsa.AESTestActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".views.aes_rsa.RSATestActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>